    <script src="~/Scripts/Datetimepicker/bootstrap-datetimepicker.js"></script>
    <script src="~/Scripts/Datetimepicker/bootstrap-datetimepicker.fr.js"></script>
    <script src="http://maps.googleapis.com/maps/api/js?key=AIzaSyBeyBqnIIC1RaRL1hXZ9f05KiRdvWKrIws&libraries=places,geometry,drawing"></script>
    <script src="~/Scripts/Gmap/v3_epoly.js"></script>
