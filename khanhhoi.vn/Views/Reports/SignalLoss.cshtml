                                <input class="form-control" type="text" value="" placeholder="Search" name="q">
                                <div class="input-group-btn">
                                    <button type="button" class="btn btn-default dropdown-toggle" data-toggle="dropdown">Select <span class="caret"></span></button>
                                    <ul class="dropdown-menu">
                                        <li><a href="#" class="small" data-value="option1" tabIndex="-1"><input type="checkbox" />&nbsp;Tất cả</a></li>
                                        <li><a href="#" class="small" data-value="option2" tabIndex="-1"><input type="checkbox" />&nbsp;29C84529</a></li>
                                        <li><a href="#" class="small" data-value="option3" tabIndex="-1"><input type="checkbox" />&nbsp;29C84561</a></li>
                                        <li><a href="#" class="small" data-value="option4" tabIndex="-1"><input type="checkbox" />&nbsp;29H13747</a></li>
