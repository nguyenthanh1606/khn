            <div class="panel-body">
                <form method="post" action=".">
                    <div id="div_id_password1" class="form-group required">
                        <label for="id_password1" class="control-label col-md-4  requiredField">Chọn thời gian</label>
                        <div class="controls col-md-8 ">
                            <div class="input-group date m-b-5">
                                <input class="form-control" type="text" name="datetimes" value="" />
                                <span class="input-group-addon">
                                    <i class="fa fa-calendar"></i>&nbsp;
                                </span>
                            </div>
                            <script>
                                $(function () {
                                    $('input[name="datetimes"]').daterangepicker({
                                        timePicker: true,
                                        startDate: moment().startOf('hour'),
                                        endDate: moment().startOf('hour').add(32, 'hour'),
                                        locale: {
                                            format: 'M/DD hh:mm A'
                                        }
                                    });
                                });
                            </script>
                        </div>
                    </div>
                    <div class="form-group">
                        <label class="control-label col-md-4"> Biển số xe <span class="text-danger">*</span></label>
